		{
			"name": "FormData",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Example form data response",
							"// Assume this response is received: \"name=John+Doe&email=john.doe%40example.com&age=30\"",
							"var res = \"name=John+Doe&email=john.doe%40example.com&age=30\";",
							"",
							"// Test to validate form data",
							"pm.test(\"Response contains valid form data\", function () {",
							"    // Get the response text",
							"    var responseBody = res;//pm.response.text();",
							"",
							"    // Split the response text into key-value pairs",
							"    var formData = {};",
							"    responseBody.split('&').forEach(function(part) {",
							"        var item = part.split('=');",
							"        formData[decodeURIComponent(item[0])] = decodeURIComponent(item[1] || '').replace(/\\+/g, ' ');",
							"    });",
							"",
							"    // Now you can validate the form data",
							"    pm.expect(formData.name).to.eql(\"John Doe\");",
							"    pm.expect(formData.email).to.eql(\"john.doe@example.com\");",
							"    pm.expect(formData.age).to.eql(\"30\");",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
